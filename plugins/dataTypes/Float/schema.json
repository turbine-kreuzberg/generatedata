{
	"title": "Float",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"min": {
			"type": "integer"
		},
		"max": {
			"type": "integer"
		},
		"decimalPoint": {
			"type": "string",
			"minLength": 1,
			"maxLength": 1,
			"pattern": "^[.,]$"
		},
		"decimalPlaces": {
			"type": "integer",
			"minimum": 0,
			"maximum": 5
		}
	},
	"required": ["max"]
}
